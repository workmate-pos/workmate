{
  "name": "@work-orders/common",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./hooks/*": {
      "default": "./dist/hooks/*",
      "types": "./src/hooks/*"
    },
    "./queries/*": {
      "default": "./dist/queries/*",
      "types": "./src/queries/*"
    },
    "./util/*": {
      "default": "./dist/util/*",
      "types": "./src/util/*"
    },
    "./batcher/*": {
      "default": "./dist/batcher/*",
      "types": "./src/batcher/*"
    },
    "./types/*": {
      "default": "./dist/types/*",
      "types": "./src/types/*"
    },
    "./custom-attributes/*": {
      "default": "./dist/custom-attributes/*",
      "types": "./src/custom-attributes/*"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w"
  },
  "author": "Tim van Dam",
  "devDependencies": {
    "@types/react": "^17.0.73",
    "@types/react-dom": "^17.0.25",
    "@types/react-query": "^1.2.9",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@teifi-digital/shopify-app-toolbox": "^0.0.14",
    "zod": "^3.22.4"
  },
  "overrides": {
    "@types/react": "^17.0.73",
    "@types/react-dom": "^17.0.25"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.39.3"
  }
}
