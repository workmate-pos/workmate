{
  "name": "@work-orders/common",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./hooks/*": {
      "default": "./dist/hooks/*",
      "types": "./src/hooks/*"
    },
    "./queries/*": {
      "default": "./dist/queries/*",
      "types": "./src/queries/*"
    },
    "./util/*": {
      "default": "./dist/util/*",
      "types": "./src/util/*"
    },
    "./batcher/*": {
      "default": "./dist/batcher/*",
      "types": "./src/batcher/*"
    },
    "./types/*": {
      "default": "./dist/types/*",
      "types": "./src/types/*"
    },
    "./custom-attributes/*": {
      "default": "./dist/custom-attributes/*",
      "types": "./src/custom-attributes/*"
    },
    "./create-purchase-order/*": {
      "default": "./dist/create-purchase-order/*",
      "types": "./src/create-purchase-order/*"
    },
    "./create-work-order/*": {
      "default": "./dist/create-work-order/*",
      "types": "./src/create-work-order/*"
    },
    "./create-special-order/*": {
      "default": "./dist/create-special-order/*",
      "types": "./src/create-special-order/*"
    },
    "./create-stock-transfer/*": {
      "default": "./dist/create-stock-transfer/*",
      "types": "./src/create-stock-transfer/*"
    },
    "./create-serial/*": {
      "default": "./dist/create-serial/*",
      "types": "./src/create-serial/*"
    },
    "./metafields/*": {
      "default": "./dist/metafields/*",
      "types": "./src/metafields/*"
    },
    "./time/*": {
      "default": "./dist/time/*",
      "types": "./src/time/*"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm -fr dist node_modules tsconfig.tsbuildinfo",
    "watch": "tsc -w"
  },
  "author": "Tim van Dam",
  "devDependencies": {
    "@types/uuid": "^9.0.8"
  },
  "dependencies": {
    "@teifi-digital/shopify-app-toolbox": "^0.0.72",
    "ts-pattern": "^5.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "overrides": {
    "@types/react": "^17.0.75",
    "@types/react-dom": "^17.0.25"
  }
}
