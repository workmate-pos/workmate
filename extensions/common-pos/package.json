{
  "name": "@work-orders/common-pos",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./hooks/*": {
      "default": "./dist/hooks/*",
      "types": "./src/hooks/*"
    },
    "./components/*": {
      "default": "./dist/components/*",
      "types": "./src/components/*"
    },
    "./providers/*": {
      "default": "./dist/providers/*",
      "types": "./src/providers/*"
    },
    "./util/*": {
      "default": "./dist/util/*",
      "types": "./src/util/*"
    }
  },
  "scripts": {
    "build:web": "cd ../../web && npm install && npm run build",
    "build:common": "cd ../../common && npm install && npm run build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "preinstall": "cd ../ && npm install",
    "set-app-url": "node ./set-app-url.js",
    "postinstall": "npm run set-app-url && npm run build:web && npm run build:common && npm run build"
  },
  "author": "Tim van Dam",
  "devDependencies": {
    "@types/node": "^20.11.17"
  },
  "dependencies": {
    "@teifi-digital/shopify-app-toolbox": "^0.0.14",
    "@work-orders/common": "file:../../common"
  }
}
