{
  "name": "@work-orders/common-pos",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./hooks/*": {
      "default": "./dist/hooks/*",
      "types": "./src/hooks/*"
    },
    "./components/*": {
      "default": "./dist/components/*",
      "types": "./src/components/*"
    },
    "./screens/*": {
      "default": "./dist/screens/*",
      "types": "./src/screens/*"
    },
    "./types/*": {
      "default": "./dist/types/*",
      "types": "./src/types/*"
    },
    "./providers/*": {
      "default": "./dist/providers/*",
      "types": "./src/providers/*"
    },
    "./util/*": {
      "default": "./dist/util/*",
      "types": "./src/util/*"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm -fr dist node_modules",
    "watch": "tsc -w"
  },
  "author": "Tim van Dam",
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@teifi-digital/shopify-app-toolbox": "^0.0.56",
    "@work-orders/common": "file:../../common",
    "uuid": "^10.0.0"
  }
}
