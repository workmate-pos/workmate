{
  "name": "@work-orders/common-pos",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./hooks/*": {
      "default": "./dist/hooks/*",
      "types": "./src/hooks/*"
    },
    "./components/*": {
      "default": "./dist/components/*",
      "types": "./src/components/*"
    },
    "./screens/*": {
      "default": "./dist/screens/*",
      "types": "./src/screens/*"
    },
    "./types/*": {
      "default": "./dist/types/*",
      "types": "./src/types/*"
    }
  },
  "scripts": {
    "build:web": "cd ../../web && npm install && npm run build",
    "build:common": "cd ../../common && npm install && npm run build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "preinstall": "cd ../ && npm install",
    "postinstall": "npm run build:web && npm run build:common && npm run build"
  },
  "author": "Tim van Dam",
  "devDependencies": {
    "@types/node": "^20.11.17",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@teifi-digital/shopify-app-toolbox": "^0.0.42",
    "@work-orders/common": "file:../../common"
  }
}
