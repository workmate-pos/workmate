    "name": {
      "type": "string",
      "minLength": 1
    },
    "address": {
      "type": "string"
    },
    "vendors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "productVariantIds": {
      "type": "array",
      "items": {
        "$ref": "scalars.json#/definitions/ID"
      }
    }
  },
  "required": ["name", "address", "vendors", "productVariantIds"],
  "additionalProperties": false
}
