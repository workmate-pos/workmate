{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "offset": {
      "type": "integer",
      "minimum": 0
    },
    "status": {
      "type": "string"
    },
    "limit": {
      "type": "integer",
      "minimum": 1,
      "maximum": 50
    },
    "query": {
      "type": "string"
    },
    "customerId": {
      "$ref": "scalars.json#/definitions/ID"
    },
    "customFieldFilters": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "type": {
      "enum": ["NORMAL", "DROPSHIP"]
    }
  },
  "required": ["limit", "offset"],
  "additionalProperties": false
}
