{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "offset": {
      "type": "integer",
      "minimum": 0
    },
    "status": {
      "enum": ["OPEN", "RECEIVED", "CANCELLED", "CLOSED"]
    },
    "limit": {
      "type": "integer",
      "minimum": 1,
      "maximum": 25
    },
    "query": {
      "type": "string"
    },
    "customerId": {
      "$ref": "scalars.json#/definitions/ID"
    }
  },
  "required": ["limit", "offset"],
  "additionalProperties": false
}
