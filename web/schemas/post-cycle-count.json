{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "locationId": {
      "$ref": "scalars.json#/definitions/ID"
    },
    "productVariants": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "$ref": "scalars.json#/definitions/ID"
          },
          "quantity": {
            "$ref": "scalars.json#/definitions/Int"
          }
        },
        "required": ["id", "quantity"]
      }
    }
  },
  "additionalProperties": false,
  "required": ["locationId", "productVariants"]
}
