{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "keys": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "default": {
      "type": "boolean"
    }
  },
  "required": ["name", "keys", "default"],
  "additionalProperties": false
}
