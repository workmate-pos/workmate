{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "scalars.json",
  "definitions": {
    "ID": {
      "title": "ID",
      "type": "string",
      "pattern": "gid://shopify/\\w+/\\d+",
      "tsType": "string & { __brand: 'ID' }"
    },
    "Money": {
      "title": "Money",
      "type": "string",
      "pattern": "^-?\\d+(?:\\.\\d+)?$",
      "tsType": "`${number}` & { __brand: 'Money' }"
    },
    "Decimal": {
      "title": "Decimal",
      "type": "string",
      "pattern": "^-?\\d+(?:\\.\\d+)?$",
      "tsType": "`${number}` & { __brand: 'Decimal' }"
    },
    "Int": {
      "title": "Int",
      "type": "integer",
      "tsType": "number & { __brand: 'Int' }"
    },
    "DateTime": {
      "description": "ISO 8601 formatted string",
      "title": "DateTime",
      "type": "string",
      "format": "date-time",
      "tsType": "string & { __brand: 'DateTime' }"
    }
  }
}
